# import pdfminer.six
# Importando a biblioteca Tabula
# import pytesseract

import tabula

# Abrindo o arquivo PDF
pdf_file = "1-OGX-59-MA_Perfil_Composto.pdf"

# Extraindo todas as tabelas do PDF
tabelas = tabula.read_pdf(pdf_file)

# Acessando a primeira tabela
primeira_tabela = tabelas[0]

# Imprimindo a primeira tabela
print(primeira_tabela)

# Salvando a primeira tabela em formato CSV
primeira_tabela.to_csv("tabela_extraida.csv")
